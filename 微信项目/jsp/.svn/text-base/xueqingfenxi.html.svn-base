	<script>
		//册别切换
		var flage = 1;
		$(".classname").click(function() {
			$(this).children('ul').slideToggle()
			if(flage == 1) {
				$(".xq_up").css("transform", "rotate(180deg)")
				flage = 2;
			} else {
				$(".xq_up").css("transform", "rotate(0deg)")
				flage = 1
			}
		})
		$(".classname ul li").click(function() {
			$(".classname span").eq(0).html($(this).html())
		})
		//头像切换
		var domList = [{
			'height': '100%',
			'width': '100%',
			'content': '<p data="0" style="font-size: 26px;"><img src="../img/1.jpg" />王小刀1</p>'
		}, {
			'height': '100%',
			'width': '100%',
			'content': '<p data="1" style="font-size: 26px;"><img src="../img/2.jpg"/>王小刀2</p>'
		}, {
			'height': '100%',
			'width': '100%',
			'content': '<p data="2" style="font-size: 26px;"><img src="../img/3.jpg"/>王小刀3</p>'
		}, {
			'height': '100%',
			'width': '100%',
			'content': '<p data="3" style="font-size: 26px;"><img src="../img/4.jpg"/>王小刀3</p>'
		}];
		var islider4 = new iSlider({
			//initIndex:1,
			data: domList,
			dom: document.getElementById("dom-effect"),
			type: 'dom',
			animateType: 'depth',
			isAutoplay: false,
			//duration: 500000,
			isLooping: true,
			onslideend: function() {
				console.log("回调")
			}
		});
		islider4.addDot();
		//目录导航
		$(document).ready(function() {
			$(".menuitem_school ul li").menu();
			$(".school").on("click", function() {
				$(this).toggleClass('schoolactvie').siblings().removeClass('schoolactvie')
			})
		});
	</script>
	<script>
		//百度图表
		var myChart = echarts.init(document.getElementById('zhishi'));
		var myChart2 = echarts.init(document.getElementById('xuexi'));
		var data_one = [55, 50, 50, 70, 60, 70]; //掌握度
		var data_two = [35, 40, 22, 21, 42, 80]; //参与度
		var xuexi = [70, 50, 50, 70, 60, 90] //学习能力图谱
		option = {
			title: {
				//text: '知识掌握图谱',
				textStyle: {
					color: '#6492FF',
					fontSize: 14,
				},
			},
			//关闭提示框
			tooltip: {
				show: false,
			},
			//图示
			legend: {
				show: false,
				data: ['掌握度', '参与度'],
				bottom: '0',
				center: '0',
			},
			textStyle: {
				color: '#6492FF',
				fontSize: 12,
			},
			radar: {
				shape: 'circle',
				splitNumber: 10, //环数
				splitArea: {
					areaStyle: {
						color: ['white', 'white'],
					},
				}, //环色
				//米字线
				axisLine: {
					show: true,
					lineStyle: {
						color: '#B3CAFF',
						width: 1,
						type: 'solid',
					},
				},
				//环形线
				splitLine: {
					lineStyle: {
						color: ['#B3CAFF', 'white', 'white', 'white', 'white',
							'white', '#B3CAFF', 'white', 'white', '#B3CAFF'
						],
						width: 1,
						type: 'dashed',
						radius: ['100', '80', '90'],
					},
				},
				radius: 110,
				indicator: [{
						name: '市场销售销售\n\n' + data_one[0] + '%' + ' / ' + data_two[0] + '%',
						max: 100
					},
					{
						name: '市场\n\n' + data_one[1] + '%' + ' / ' + data_two[1] + '%',
						max: 100
					},
					{
						name: '市场asd\n\n' + data_one[2] + '%' + ' / ' + data_two[2] + '%',
						max: 100
					},
					{
						name: '阿萨德\n\n' + data_one[3] + '%' + ' / ' + data_two[3] + '%',
						max: 100
					},
					{
						name: '市场5\n\n' + data_one[4] + '%' + ' / ' + data_two[4] + '%',
						max: 100
					},
					{
						name: '市场6\n\n' + data_one[5] + '%' + ' / ' + data_two[5] + '%',
						max: 100
					}
				]
			},
			series: [{
				name: '掌握度vs参与度',
				type: 'radar',
				data: [{
						value: data_one,
						name: '掌握度',
						tooltip: {
							position: ['10%', '50%']
						},
						//单项线条标志
						itemStyle: {
							normal: {
								color: '#0066CC',
								borderWidth: 0,
								opacity: 0,
							}
						},
						//单线条颜色
						lineStyle: {
							normal: {
								color: '#0066CC',
								width: 0,
							}
						},
						//单区域颜色
						areaStyle: {
							normal: {
								color: '#0066CC',
								opacity: 0.25,
							}
						},
					},
					{
						value: data_two,
						name: '参与度',
						//单线条颜色
						lineStyle: {
							normal: {
								color: '#FF6666',
								width: 1,
							}
						},
						//单项线条标志
						itemStyle: {
							normal: {
								color: '#FF6666',
								borderWidth: 1,
								opacity: 1,
							}
						},
					}
				]
			}]
		};
		myChart.setOption(option);
		optiontwo = {
			title: {
				//text:'学习能力图谱',
				textStyle: {
					color: '#6492FF',
					fontSize: 14,
				},
			},
			//关闭提示框
			tooltip: {
				show: false,
			},
			//图示
			legend: {
				show: false,
				data: ['掌握度'],
				bottom: '0',
				center: '0',
			},
			textStyle: {
				color: '#6492FF',
				fontSize: 12,
			},
			radar: {
				splitArea: {
					areaStyle: {
						color: ['white', 'white']
					}
				},
				shape: 'circle',
				splitNumber: 10,
				//米字线
				axisLine: {
					show: true,
					lineStyle: {
						color: '#B3CAFF',
						width: 1,
						type: 'solid',
					},
				},
				//环形线
				splitLine: {
					lineStyle: {
						color: ['#B3CAFF', 'white', 'white', 'white', 'white',
							'white', '#B3CAFF', 'white', 'white', '#B3CAFF'
						],
						width: 1,
						type: 'dashed'
					},
				},
				radius: 110,
				indicator: [{
						name: '数学抽象\n\n' + xuexi[0] + '%',
						max: 100
					},
					{
						name: '推理\n能力\n\n' + xuexi[1] + '%',
						max: 100
					},
					{
						name: '数据\n分析\n\n' + xuexi[2] + '%',
						max: 100
					},
					{
						name: '数学应用\n' + xuexi[3] + '%',
						max: 100
					},
					{
						name: '直观\n想象\n\n' + xuexi[4] + '%',
						max: 100
					},
					{
						name: '数学\n运算\n\n' + xuexi[5] + '%',
						max: 100
					}
				]
			},
			series: [{
				type: 'radar',
				//areaStyle: {normal: {}},
				data: [{
					value: xuexi,
					name: '掌握度',
					tooltip: {
						position: ['10%', '50%']
					},
					//单项线条标志
					itemStyle: {
						normal: {
							color: '#0066CC',
							borderWidth: 0,
							opacity: 0,
						}
					},
					//单线条颜色
					lineStyle: {
						normal: {
							color: '#0066CC',
							width: 0,
						}
					},
					//单区域颜色
					areaStyle: {
						normal: {
							color: '#0066CC',
							opacity: 0.25,
						}
					},
				}, ]
			}]
		};
		myChart2.setOption(optiontwo);
	</script>

