.tipes{ position:relative;}
.tipes .content{ width:552px; margin-left: -280px; z-index: 1000000; height:451px; margin-top: -226px; position: fixed; top: 50%; left: 50%; background: url(../images/tipes_bg.png) top center;}
.tipes .content h3{ font-size: 18px; font-family: "微软雅黑"; color: white; margin-top: 45px; margin-left: 90px;}
.tipes .content i {
	-webkit-transition: -webkit-transform .5s ease-out;
	-moz-transition: -moz-transform .5s ease-out;
	-o-transition: -o-transform .5s ease-out;
	-ms-transition: -ms-transform .5s ease-out;
	cursor: pointer;
	background: url(../../common/images/u20.png) top center;
	background-size: cover;
	display: inline-block;
	width: 30px;
	height: 30px;
	position: absolute;
	right: 0px;
	font-size: 18px;
	color: white;
}
.tipes .content i:hover {
	-webkit-transform: rotateZ(90deg);
	-moz-transform: rotateZ(90deg);
	-o-transform: rotateZ(90deg);
	-ms-transform: rotateZ(90deg);
	transform: rotateZ(90deg);
}
.tipes .content .check{ font-size: 14px; color: #666666; padding-left: 70px; padding-top: 40px; overflow: hidden;}
.tipes .content .check:after{ content:""; clear: both; display: block; zoom: 1; }
.tipes .content .check li{ line-height: 36px; padding-left: 5px;}
.tipes .content .check .left{ float: left; width: 190px;  margin-right: 10px;}
.tipes .content .check .left ul{ height: 240px; border: 1px solid #F2F2F2; padding:5px;}
.tipes .content .check .left ul .active{background-color: #00CCFF; color:white}
.tipes .content .check .left ul li:hover{ background-color: #00CCFF; color: white; cursor: pointer;}
.tipes .content .check .right{ float: left; width: 210px;}
.tipes .content .check .right ul{ height: 240px; border: 1px solid #F2F2F2; padding:5px;}
.tipes .content .check .right ul .active{background-color: #00CCFF; color:white}
.tipes .content .check .right ul li:hover{ background-color:#00CCFF; color: white; cursor: pointer;}
.tipes .content  span{ cursor: pointer; float:right;  width: 80px; hanging-punctuation: 30px; background-color: #1199FF; border: 1px solid #f1f1f1; color: white; border-radius: 15px; margin-top: 10px; margin-right: 40px; text-align: center;}
.tipes .content  span:hover{ background-color:#00CCFF;}


