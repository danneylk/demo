/*人数超20个*/
.Morethan20 .items{
	float: left;
	width: 80px;
	background:rgba(255,255,255,0.95);
	text-align: center;
	border-radius:3px ;
	box-shadow: 0px 3px 10px #666666;
	margin: 15px;
	padding:4px 0 ;
	position: relative;
}
.Morethan20 .items.active{
	box-shadow: 0px 5px 20px #FFB024;
	border: 2px solid #F49F00;
	margin: 7px;
	width: 96px;
	padding: 2px 0;
}
.Morethan20 .items.active img{
	height: 80px;
	width: 80px;
}
.Morethan20 .items.active .bg-black{
	display: none;
}
.Morethan20 .items.active p{
	color: #F49F00;
	font-weight: 700;
	width: 90px;
	text-align: center;
	height: 28px;
}
.Morethan20 .items img{
	height:64px;
	width: 64px;
	border: 4px solid #FFFFFF;
	border-radius:50% ;
}
.Morethan20 .items .header{
	position: relative;
}
.Morethan20 .items .bg-black{
	position: absolute;
	top:8px;
	left:8px;
	height:64px;
	width: 64px;
	border-radius:50% ;
	background:rgba(0,0,0,.4);
}
.Morethan20 .items  p{
	color: #666666;
	font-size: 14px;
	width: 80px;
	height: 28px;
	overflow: hidden;
}
.Morethan20 .ignore{
	background: #CCCCCC;
}
.Morethan20 .ignore img{
	border-color: #DDDDDD;
}
/*超过40个*/
