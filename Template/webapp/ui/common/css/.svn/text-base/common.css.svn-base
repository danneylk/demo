/*颜色*/
/*主要颜色*/
.c-primary,.c-primary a,a.c-primary{color:#5a98de}
.c-primary a:hover,a.c-primary:hover{ color:#5a98de}

/*次主色*/
.c-secondary,.c-secondary a,a.c-secondary{color:#555}
.c-secondary a:hover,a.c-secondary:hover{ color:#555}

/*强调色—成功*/
.c-success,.c-success a,a.c-success{color:#5eb95e}
.c-success a:hover,a.c-success:hover{ color:#5eb95e}

/*强调色—危险*/
.c-danger,.c-danger a,a.c-danger{color:#dd514c}
.c-danger a:hover,a.c-danger:hover{ color:#dd514c}

/*强调色—警告*/
.c-warning,.c-warning a,a.c-warning{color:#f37b1d}
.c-warning a:hover,a.c-warning:hover{ color:#f37b1d}

/*强调色—错误*/
.c-error,.c-error a,a.c-error{color:#c00}
.c-error a:hover,a.c-error:hover{ color:#c00}

/*辅助色—浅黑*/
.c-333,.c-333 a,a.c-333{color:#333}
.c-333 a:hover,a.c-333:hover{ color:#333}

/*辅助色—灰色*/
.c-666,.c-666 a,a.c-666{color:#666}
.c-666 a:hover,a.c-666:hover{ color:#666}
.c-999,.c-999 a,a.c-999{color:#999}
.c-999 a:hover,a.c-999:hover{color:#999}

/*标准色—红色*/
.c-red,.c-red a,a.c-red{color:#ff4949}
.c-red a:hover,a.c-red:hover{ color:#ff4949}

/*标准色—绿色*/
.c-green,.c-green a,a.c-green{color:#13ce66}
.c-red a:hover,a.c-red:hover{color:#13ce66}

/*标准色—蓝色*/
.c-blue,.c-blue a,a.c-blue{color:#20a0ff}
.c-blue a:hover,a.c-blue:hover{color:#20a0ff}

/*标准色—白色*/
.c-white,.c-white a,a.c-white{color:white}
.c-white a:hover,a.c-white:hover{color:white}

/*标准色—黑色*/
.c-black,.c-black a{color:black}
.c-black a:hover,a.c-black:hover{color:black}

/*标准色—橙色*/
.c-orange,.c-orange a,a.c-orange{color:orange}
.c-orange a:hover,a.c-orange:hover{color:orange}



/*==========================================================================================*/
/*定义最大容器，可定义width设置最大宽度，不定义则显示为100%*/
.hp-wrap { margin: 0 auto; min-width: 1200px; }
/*header*/
header { min-width: 1200px; height: 80px; line-height: 80px; background: #0066cc; color: #fff; }

header h1 {
	margin-left: 20px;
	padding-left: 60px;
	font-size: 20px;
	background: url(../images/logo.png) no-repeat left center/48px;
}

.hp-message {}

.hp-user {
	display: inline-block;
	padding-right: 25px;
	padding-left: 30px;
	background: url(../images/u2.png) no-repeat left center/20px;
}
.hp-user:hover {
	color: #ffff66;
	background-image: url(../images/u2_mouseOver.png);
}
.hp-message a {
	display: inline-block;
	margin-right: 20px;
	width: 24px;
	height: 80px;
	vertical-align: middle;
}
.hp-help {
	background: url(../images/u4.png) no-repeat left center/24px;
}
.hp-help:hover {
	background-image: url(../images/u4_mouseOver.png);
}
.hp-exit {
	background: url(../images/u6.png) no-repeat left center/24px;
}
.hp-exit:hover {
	background-image: url(../images/u6_mouseOver.png);
}


/*menu*/

.hp-left {
	margin-top: 0px;
	width: 188px;
	min-height: 645px;
	/*border: solid 1px #e4e4e4;*/
	background: #fff;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
	overflow: hidden;
	vertical-align: top;
	margin-left: 10px
}

.hp-left h2 {
	height: 45px;
	background: url(../images/u79.png) no-repeat 10px center #4f5866;
	background-size: 20px;
	color: #FFFFFF;
}

.hp-left-menu {
	padding-bottom: 50px;
	line-height: 3;
}

.hp-tree-layout {
	float: left;
	width: 410px;
	min-height: 645px;
	border: 1px solid #e4e4e4;
	background: #ffffff;
	box-sizing: border-box;
	border-radius: 4px 4px 0 0;
}

.hp-tree-layout p {
	float: left;
	width: 100%;
	background: #4f5866;
	color: #ffffff;
	line-height: 45px;
	height: 45px;
	padding-left: 10px;
	box-sizing: border-box;
	border-radius: 4px 4px 0 0;
}

.hp-tree-layout .hp-tree {
	float: left;
	min-width: 410px;
	min-height: 40px;
	padding: 10px;
	box-sizing: border-box;
}

.hp-tree-layout .tree-search {
	width: 190px;
	height: 30px;
	text-align: center;
	padding: 8px 10px;
	background: #none;
}

.hp-menu-item {
	border-bottom: solid 1px #e4e4e4;
	background: url(../../ht/images/btn-gongchengguanli.png) no-repeat 15px 12px/20px;
}
.hp-menu-item:nth-child(2) {
	background: url(../../ht/images/btn-chanpin.png) no-repeat 15px 12px/20px;
}
.hp-menu-item:nth-child(3) {
	background: url(../../ht/images/btn-kefu.png) no-repeat 15px 12px/20px;
}
.hp-menu-item:last-child {
	border: none;
	background: url(../../ht/images/btn-peixun.png) no-repeat 15px 12px/20px;
}
.hp-menu-item h3 {
	font-size: 14px;
	font-weight: normal;
	text-indent: 50px;
	background: url(../images/btn-gongc) no-repeat right 5px center/16px;
	cursor: pointer;
}

.hp-menu-item h3.hp-menu-show {
	background: #fafafa url(../images/xxjt.png) no-repeat right 5px center/16px;
}

.hp-menu-item h3.hp-menu-hide {
	background: #fafafa url(../images/xsjt.png) no-repeat right 5px center/16px;
}

.hp-menu-item h3:hover {
	color: #46a8bf;
}

.hp-menu-item li {
	text-indent: 60px;
	border-right: solid 3px transparent;
}

.hp-menu-item li a {
	display: block;
}

.hp-menu-item li:hover,
.hp-menu-item li.active {
	background: #ebf2fc;
	border-right-color: #5b9ce8;
}
.hp-menu-item li:hover a,
.hp-menu-item li.active a {
	color: #000;
}
.hp-scroll-top {
	position: fixed;
	top: 0px;
	margin-top: 0px;
}

/*右侧*/
.hp-right {
	padding: 0px 10px 20px 0px;
}

.hp-right-1 {
	padding: 0px 10px 20px 420px;
}

.hp-pannel {
	border: 1px solid #E4E4E4;
	background-color: #fff;
	padding: 24px 68px;
	margin-top: 10px;
}

.hp-pannel li {
	list-style: none;
	width: 100%;
	padding-bottom: 10px;
}

.hp-pannel li>h4 {
	color: #0066CC;
	font-size: 16px;
	font-weight: 400;
	margin-top: 4px;
	margin-bottom: 4px;
}

.hp-pannel li>h4>span {
	font-size: 12px;
	color: #FF3333;
	text-align: left;
	line-height: 20px;
	font-weight: 400;
	background: url(../images/warn_tip_bg.png) no-repeat;
	height: 20px;
	display: inline-block;
	min-width: 200px;
	padding-left: 12px;
	margin-left: 16px;
}

.hp-pannel li>h4>span>img {
	display: inline-block;
	width: 14px;
	height: 14px;
	margin-bottom: 3px;
	margin-right: 8px;
}

.hp-pannel em {
	font-size: 14px;
	color: #FF6666;
	margin: 0 5px;
	font-weight: bold;
	font-style: normal;
}


/*右侧操作区*/

.hp-operateBox {
	border: 1px solid #eaeefb;
	background-color: #fff;
	margin-bottom: 10px;
	padding: 16px 20px 10px 20px;
}
.hp-operateBox:hover{
	box-shadow: 0 0 8px #eaeefb;
}
.hp-operateBox .o-title>label {
	font-weight: 400;
	font-family: "microsoft yahei";
	font-size: 18px;
	text-align: left;
	line-height: 20px;
	color: #333333;
}

.hp-operateBox .o-title>span {
	font-size: 14px;
	color: #FF6666;
	padding: 0 0 0 20px;
	margin-left: 10px;
	background: url(../images/btn-jinggao.png) no-repeat 4px center;
	background-size: 16px;
}

.hp-operateBox .o-title>.return {
	cursor: pointer;
	float: right;
	font-size: 14px;
	color: #666666;
	background: url(../images/btn-fanhui.png) left center/18px no-repeat;
	padding-left: 24px;
}

.hp-operateBox .o-title>.return:hover {
	color: #006699;
	background: url(../images/btn-fanhui.png) left center/18px no-repeat;
	padding-left: 24px;
	text-decoration: underline;
}

.hp-operateBox .splitBar {
	border-bottom: 1px solid #eaeefb;
	margin-top: 5px;
}

.hp-operateBox .o-btnBox {
	min-height: 54px;
	padding-top: 16px;
	min-width: 1000px;
}

.hp-operateBox .o-btnBox p {
	float: left;
	margin-right: 10px;
}

.hp-operateBox #moreSearch {
	cursor: pointer;
	padding-right: 16px;
	width: 62px;
	font-size: 12px;
	background: url(../images/btn-arrow-down.png) right center no-repeat;
	background-size: 10px;
}

.hp-operateBox #moreSearch:hover {
	color: #0066CC
}

.hp-operateBox .more {
	background: url(../images/btn-arrow-up.png) right center no-repeat !important;
	background-size: 10px !important;
}

.hp-operateBox .moreSearch {
	display: none;
}

.hp-content {
	min-height: 645px;
}

.hp-content-head {
	padding: 10px 20px;
	border: solid 1px #e4e4e4;
	background: #fff;
}

.hp-content-head .hp-content-title .return {
	cursor: pointer;
	float: right;
	font-size: 14px;
	color: #666666;
	background: url(../../ht/images/btn-fanhui.png) left center/18px no-repeat;
	padding-left: 24px;
}

.hp-content-item {
	border-radius: 5px;
	overflow: hidden;
	border: solid 1px #4f5866;
	border-right: none;
	overflow: hidden;
}

.hp-content-item a {
	display: block;
	width: 80px;
	height: 30px;
	float: left;
	border-right: solid 1px #4f5866;
	text-align: center;
	background: #f2f2f2;
}

.hp-content-item a.active,
.hp-content-item a.active:hover {
	color: #fff;
	background: #4f5866;
}

.hp-content-item a:hover {
	color: initial;
	background: #e4e4e4;
}

.hp-search-input,
.hp-search-btn {
	border: solid 1px #4f5866;
	height: 30px;
	box-sizing: border-box;
}

.hp-search-input {
	width: 160px;
}

.hp-search-input+.hp-search-btn {
	border-left: none;
}

.hp-search-input {
	padding: 0px 5px;
}

.hp-search-btn {
	width: 30px;
	background: #e4e4e4 url(../images/btn-chazhao.png) no-repeat center/16px;
}

.hp-search-btn:hover {
	width: 30px;
	background: #f2f2f2 url(../images/btn-chazhao.png) no-repeat center/20px;
	transition: all 0.1s ease 0s;
}

.hp-input-date {
	width: 210px;
}


/*颜色*/

.pink,
.pink:hover {
	color: #f66 !important;
}

.orange {
	color: #f90 !important;
}

.blue {
	color: #5B9CE8;
}

.lowblue {
	color: #0066CC;
}

.lowgrey {
	color: #999999;
}

.lowblack {
	color: #666666;
}


/*间隔*/

.mr-20 {
	margin-right: 20px;
}

.mt-4 {
	margin-top: 4px;
}

.mt-20 {
	margin-top: 20px;
}

.mt-10 {
	margin-top: 10px;
}

.ml-10 {
	margin-left: 10px;
}

li {
	box-sizing: border-box;
}


/*列表样式一  开始*/

.hp-list {
	padding: 10px 20px;
	min-height: 520px;
	border: solid 1px #e4e4e4;
	background: #fff;
}

.hp-list-cz {
	display: inline-block;
}

.hp-list-cz .wd-check {
	margin: 6px 10px 0 0;
}

.hp-list-count {
	position: relative;
}

.hp-list-count span {
	color: #666666;
}

.hp-list-check {
	position: absolute;
	width: 20px;
	float: left;
	top: 14px;
}

.hp-list-head {
	height: 57px;
	padding: 10px 0 0 0;
	border-bottom: solid 2px #4f5866;
}


/*列表样式 - 筛选*/

.hp-list-filter {
	margin-left: 10px;
	display: inline-block;
	text-indent: 25px;
	background: url(../images/btn-filter.png) no-repeat left center/16px;
}

.hp-list-filter:hover {
	background-image: url(../images/btn-filter_hover.png);
	color: initial;
}

.hp-filter {
	display: none;
	height: 66px;
	position: absolute;
	right: -10px;
	top: 45px;
	background: #FFF;
	border: solid 2px #4f5866;
	padding: 15px 0px 15px 10px;
	z-index: 2;
	width: 340px;
}

.hp-filter input[type="text"] {
	margin-right: 10px;
	width: 125px;
	box-sizing: border-box;
	color: #666;
	border: solid 1px #ccc;
	height: 30px;
}

.hp-filter-btn {
	width: 52px;
	height: 30px;
	color: #ffffff;
	border-radius: 3px;
	background: #5b9ce8;
	border: solid 1px #5b9ce8;
}

.hp-filter-btn:hover {
	box-shadow: 1px 1px 1px 1px #ccc;
}

.hp-filter-btn-clear {
	width: 52px;
	height: 30px;
	border-radius: 3px;
	background: #f2f2f2;
	border: solid 1px #e4e4e4;
}

.hp-filter-btn-clear:hover {
	box-shadow: 1px 1px 1px 1px #ccc;
}


/*列表样式 - 行样式*/

.hp-list-row {
	list-style: none;
	padding: 10px 0px;
	border-bottom: dashed 1px #ccc;
	position: relative;
	min-width: 1000px;
}

.hp-list-row:nth-child(odd) {
	background-color: #fafafa;
}

.hp-list-row.odd {
	background-color: #fffbf3;
}

.hp-list-row:hover {
	background-color: #fffbf3;
}

.hp-list-row dl {
	width: 100%;
	display: block;
	margin-left: 30px;
}

.hp-list-row dl.no-checkbox {
	margin-left: 0px;
}

.hp-list-row p {
	float: left;
	color: #999999;
	margin-right: 40px;
}

.hp-list-row .mainTxt {
	font-family: "微软雅黑";
	color: #000000;
	cursor: pointer;
}

.hp-list-row dl p a {
	max-width: 600px;
	display: block;
	height: 24px;
}

.hp-list-row .mainTxt:hover {
	color: #0099CC;
	text-decoration: none;
	border-bottom: 1px solid;
}


/*列表样式：按钮*/

.hp-list-row .hp-list-operate {
	position: absolute;
	right: 0;
	top: 10px;
	height: 60px;
	display: inline-block;
}

.hp-list-operate p {
	margin: 0;
	cursor: pointer;
}

.hp-list-operate p.btn-ico {
	padding: 0px 0px 0 30px;
	width: 0px;
	overflow: hidden;
	height: 30px;
	display: block;
	background-size: 18px;
	background-position: 6px 6px;
	background-repeat: no-repeat;
	float: left;
}

.hp-list-operate p.btn-ico:hover {
	background-color: #5b9ce8;
	color: #ffffff;
	border-radius: 15px;
	transition: background-color 0.2s ease 0s;
}

.hp-list-operate .btn-ico-text {
	padding: 2px 10px 0 30px;
	height: 30px;
	display: block;
	background-size: 18px;
	background-position: 6px 6px;
	background-repeat: no-repeat;
	float: left;
}

.hp-list-operate .btn-ico-text:hover {
	background-color: #5b9ce8;
	color: #ffffff;
	border-radius: 15px;
	display: block;
	float: left;
	transition: background-color 0.5s ease 0s;
}


/*列表样式：分页*/

.hp-page {
	padding: 20px 0px;
	text-align: right;
	overflow: hidden;
}

.hp-page span,
.hp-page a {
	display: inline-block;
	width: 32px;
	height: 32px;
	line-height: 32px;
	border-radius: 2px;
