html,body { background: #FFFFFF; padding: 0; margin: 0; font-size: 14px; line-height: 2; font-family: 'Microsoft YaHei Regular', 'Microsoft YaHei'; }
 h1, h2, h3, h4, h5, h6, dl, dt, dd, ul, ol, li, th, td, p, span, 
 blockquote, pre, form, fieldset, legend, input, button, textarea, hr { padding: 0; margin: 0; }
 img, select, input, button { vertical-align: middle; }
 a, input { outline: none; font-family: 'Microsoft YaHei Regular', 'Microsoft YaHei'; }
 a { text-decoration: none; outline-style: none !important; -moz-outline-style: none !important; color: #333; }
 a:hover { color: #0066CC; }
 ul, ul li, ol { list-style: none; }
 table { border-spacing: 0; border-collapse: collapse; }
textarea { border: 1px solid #CCC; resize: none; overflow-x: hidden; overflow-y: auto; resize: none; }
/*清除浮动*/
.clear:after, ._clear:after { content: ""; height: 0px; line-height: 0px; visibility: hidden; display: block; clear: both; }
._clear { overflow: hidden; }
.none { display: none; }

/*文字尺寸*/
.f-12{font-size:12px}
.f-14{font-size:14px}
.f-16{font-size:16px}
.f-18{font-size:18px}
.f-20{font-size:20px}
.f-24{font-size:24px}
.f-26{font-size:26px}
.f-28{font-size:28px}
.f-30{font-size:30px}

/*左右浮动*/
.fl { float: left; }
.fr { float: right; }


/*定位*/
.pos-r{position:relative}
.pos-a{position:absolute}
.pos-f{position:fixed}
/*绝对定位位置*/
.left{ left:0; right:auto}
.right{ right:0; left:auto}
.top{ top:auto; bottom:0}
.bottom{ top:0; bottom:auto}

/*行内分割线*/
.pipe{margin:0 5px;color:#CCC}

/*h1,h2…*/
h1,h2,h3,h4,h5,h6{font-weight:500;color:inherit}
h1 small,h2 small,h3 small,h4 small,h5 small,h6 small,h1 .small,h2 .small,h3 .small,h4 .small,h5 .small,h6 .small{font-weight:normal;line-height:1;color:#999}
/*h1,h2,h3{margin-top:20px;margin-bottom:10px}*/
h1 small,h2 small,h3 small,h1 .small,h2 .small,h3 .small{font-size:65%}
h4,h5,h6{margin-top:10px;margin-bottom:10px}
h4 small,h5 small,h6 small,h4 .small,h5 .small,h6 .small{font-size:75%}
h1{font-size:36px}
h2{font-size:30px}
h3{font-size:24px}
h4{font-size:18px}
h5{font-size:14px}
h6{font-size:12px}
/*文字单行溢出省略号*/
.text-overflow{overflow: hidden;text-overflow: ellipsis;white-space: nowrap}

/*单条线*/
.line{ font-size: 0px; line-height: 0px; border-top: solid 1px #ddd; float: none}
img { border: 0; }
input[type="text"],
textarea {
	font-family: 'Microsoft YaHei Regular', 'Microsoft YaHei';
}

input[type="button"] {
	cursor: pointer;
}
div { box-sizing: border-box; }
input[type="text"]:disabled {
	background-color: #f2f2f2;
	border-color: #cccccc;
}


/*外边距定义*/
.mt-5{margin-top:5px}/*距上5像素*/
.mt-10{margin-top:10px}/*距上10像素*/
.mt-15{margin-top:15px}/*距上15像素*/
