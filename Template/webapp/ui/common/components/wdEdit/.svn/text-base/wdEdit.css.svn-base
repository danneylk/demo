*,
*:before,
*:after {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
ul, li, lo{
	list-style: none;
}
.wd-editNone{
	display: none;
}
.wd-editbox{
	border: 1px solid #CCC;
	border-radius: 2px;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
}
.wd-edittoolbox{
	border-bottom: 1px solid #CCC;
	height: 34px;
	padding: 5px;
	width: 100%;
	background: #F6F6F6;
}
.wd-editeditbox{
	overflow-y: auto;
	cursor: text;
}
.wd-edittoolbox .wd-toolbox{
	float: left;
	border: 1px solid transparent;
	width: 24px;
	height: 24px;
	line-height: 22px;
	margin-right: 2px;
	text-align: center;
	cursor: default;
	overflow: hidden;
}
.wd-edittoolbox-active, .wd-toolbox:hover{
	border: 1px solid #dcac6c !important;
    border-radius: 2px;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    background-color: #FFE69F;
    padding: 0;
}
.wd-bold{
	font-weight: 700;
}
.wd-italic{
	font-style: italic;
}
.wd-edittoolbox .wd-insertimages{
	display: block;
	background: url(images/insertimages.png) no-repeat center center;
	position: relative;
}
.wd-edittoolbox .wd-insertimage{
	display: block;
	background: url(images/insertimage.png) no-repeat center center;
	position: relative;
}
.wd-edittoolbox .wd-insertimages:hover,
.wd-edittoolbox .wd-insertimage:hover{
	border: 1px solid #dcac6c;
    border-radius: 2px;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    background-color: #FFE69F;
    padding: 0;
}
.wd-edittoolbox .wd-toolbox-default{
	opacity: 0.3;
}
.wd-edittoolbox .wd-toolbox-default:hover{
	border: 1px solid transparent !important;
    border-radius: 2px;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    background-color: transparent;
    padding: 0;
}
.wd-edittoolbox .wd-underline{
	text-decoration: underline;
}
.wd-edittoolbox .wd-formula{
	display: block;
	position: relative;
}
.wd-insertimage input[type="file"]{
	width: 20px;
	height: 20px;
}
.wd-editBg{
	background: #333;
	display: none;
	width: 100%;
	height: 100%;
	position: absolute;
	left: 0;
	top: 0;
	z-index: 99;
	opacity: 0.3;
	filter: alpha(opacity=30);
	-moz-opacity: 0.3;
}
.wd-editChosenWrap{
	border: 1px solid #CCC;
	border-radius: 5px;
	-webkit-border-radius: 5px;
    -moz-border-radius: 5px;
	background: #FFF;
	display: none;
	width: 650px;
	height: 470px;
	position: fixed;
	left: 50%;
	top: 50%;
	margin-left: -325px;
	margin-top: -235px;
	z-index: 100;
}
.wd-editChosenTop{
	border-bottom: 1px solid #CCC;
	background: #F6F6F6;
	height: 26px;
	line-height: 26px;
	font-size: 12px;
	padding-left: 10px;
	padding-right: 5px;
}
.wd-editChosenTit{
	float: left;
	font-weight: 700;
}
.wd-editChosenClose{
	float: right;
	display: block;
	background: url(images/close.png) no-repeat center center;
	width: 22px;
	height: 25px;
	cursor: pointer;
}
.wd-editChosenClose:hover{
	background: url(images/close_hover.png) no-repeat center center;
	width: 20px;
	height: 25px;
}
.wd-editChosenCont{
	padding: 10px;
}
.wd-editChosen{
	border: 1px dashed #CCC;
	width: 630px;
	height: 380px;
	position: relative;
}
.wd-editUploadWrap{
	display: block;
	width: 170px;
	height: 110px;
	position: absolute;
	left: 50%;
	top: 50%;
	margin-left: -85px;
	margin-top: -55px;
}
.wd-editUploadImage{
	margin: 0 auto;
	direction: block;
	background: url(images/image.png) no-repeat center center;
	width: 60px;
	height: 60px;
}
.wd-editChosenPic{
	border: 0;
	border-radius: 5px;
	-webkit-border-radius: 5px;
    -moz-border-radius: 5px;
	background: #00B7EE;
	display: block;
	overflow: hidden;
	width: 170px;
	height: 40px;
	line-height: 40px;
	margin-top: 14px;
	font-size: 16px;
	color: #FFF;
	text-align: center;
	cursor: pointer;
	position: relative;
}
.wd-file{
	overflow: hidden;
	font-size: 100px;
	position: absolute;
	top: 0;
	right: 0;
	cursor: pointer;
	opacity: 0;
	filter: alpha(opacity=0);
	-moz-opacity: 0;
}
.wd-editChosenPic input[type=file]{
	width: 170px;
	height: 40px;
}
.wd-editChosenPic:hover{
	background: #00A2D4;
}
.wd-editFileWrap{
	width: 630px;
	height: 380px;
}
.wd-editFileTop{
	overflow: hidden;
	border-bottom: 1px solid #E4E4E4;
	height: 40px;
	line-height: 30px;
	padding-bottom: 10px;
	font-size: 14px;
}
.wd-editFileDesc{
	float: left;
}
.wd-editDescWsc{
	display: none;
	color: #FF6600;
}
.wd-editFileOpt{
	float: right;
}
.wd-editFileBtn{
	border: 1px solid #CCC;
	border-radius: 5px;
	-webkit-border-radius: 5px;
    -moz-border-radius: 5px;
	background: #FFF;
	width: 80px;
	height: 30px;
	line-height: 28px;
	margin-left: 5px;
	padding: 0;
	font-size: 14px;
	text-align: center;
	cursor: pointer;
}
.wd-editAdd{
	position: relative;
	display: inline-block;
}
.wd-editAdd input[type=file]{
	width: 80px;
	height: 28px;
}
.wd-editAdd:hover, .wd-editAdd:focus{
	background: #F0F0F0;
}
.wd-editUpload{
	border: 1px solid #00B7EE;
	background: #00B7EE;
	color: #FFF;
}
.wd-editUpload:hover, .wd-editUpload:focus{
	border: 1px solid #00A2D4;
	background: #00A2D4;
}
.wd-editFileList{
	margin: 0 0 0 -10px;
	padding: 0;
	overflow-y: auto;
	height: 340px
}
.wd-editFileList li{
	float: left;
	border: 1px solid #E4E4E4;
	display: block;
	width: 113px;
	height: 113px;
	line-height: 113px;
	font-size: 0;
	text-align: center;
	margin: 9px 0 0 9px;
	position: relative;
	overflow: hidden;
}
.wd-editFileList li img{
	max-width: 100%;
	max-height: 100%;
	vertical-align: middle;
}
.wd-editFiles{
	float: left;
	display: inline-block;
}
.wd_editFileSuccess{
	display: none;
	background: url(./images/success.png) no-repeat right bottom;
    position: absolute;
    left: 0;
    bottom: 0;
    height: 40px;
    width: 100%;
    z-index: 100;
}
.wd-editFileSingleOpt{
	/*display: none;*/
	background: rgba(0,0,0,0.5);
	width: 100%;
	height: 30px;
	overflow: hidden;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 100;
}
.wd-editFileDel{
	float: right;
	background: url(./images/icons.png) no-repeat -48px -24px;
	display: inline-block;
	width: 24px;
	height: 24px;
	margin: 5px 2px 0 1px;
	cursor: pointer;
}
.wd-editFileDel:hover{
	background-position: -48px 0;
}
.wd-editFileAddBox{
	background: url(images/image.png) no-repeat center center;
	cursor: pointer;
}
.wd-editFileAddBox input[type=file]{
	width: 113px;
	height: 113px;
}
.wd-editChosenBottom{
	padding-top: 12px;
	text-align: right;
}
.wd-editChosenBtn{
	border: 1px solid #CCC;
	border-radius: 5px;
	-webkit-border-radius: 5px;
    -moz-border-radius: 5px;
	background: #F3F3F3;
	display: inline-block;
	width: 80px;
	height: 26px;
	line-height: 22px;
	margin-left: 5px;
	font-size: 12px;
	text-align: center;
	cursor: pointer;
}
.wd-editChosenBtn:hover{
	background: #FFE69F;
}
.wd-editeditbox{
	padding: 8px;
}
.wd-editeditbox img{
	vertical-align: bottom !important;
}
.edit-popup-cont{
	display: none;
	background: #FFF6D9;
	border: 1px solid #DCAC6C;
	border-radius: 6px;
	 -webkit-border-radius: 6px;
    -moz-border-radius: 6px;
    padding: 0 5px;
    width: 100px;
    height: 30px;
    line-height: 30px;
    font-size: 12px;
    position: absolute;
    font-weight: 500;
    z-index: 100;
}
.wd-editTable{
	border-spacing: 0;
    border-collapse: collapse;
}
.wd-editTable tr{
	font-size: 14px;
	height: 40px;
	line-height: 40px;
}
.wd-editTable th{
	width: 80px;
	padding-right: 5px;
	font-weight: 100;
	text-align: right;
}
.wd-editInput{
	border: 1px solid #CCC;
	border-radius: 2px;
	-webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
	width: 180px;
	height: 30px;
	line-height: 28px;
	padding: 0 4px;
}
.wd-editImg{
	padding-top: 20px;
}
.wd-editImgWrap{
	position: relative;
	display: inline-block;
}
.wd-eidtImgDel{
	float: left;
	background: url(./images/icons.png) no-repeat -48px -24px;
	display: inline-block;
	width: 20px;
	height: 24px;
	margin-top: 5px;
	margin-right: 5px;
	margin-left: 8px;
	cursor: pointer;
}
.wd-eidtImgDel:hover{
	background-position: -48px 0;
}
.wd-editImgDown{
	float: left;
	background: url(images/arrowDown.png) no-repeat center center;
	display: inline-block;
	width: 20px;
	height: 24px;
	margin-top: 1px;
	margin-right: 5px;
	cursor: pointer;
}
.wd-editImgDown:hover{
	background-image: url(images/arrowDown_hover.png);
}
.wd-editImgUp{
	float: left;
	background: url(images/arrowUp.png) no-repeat center center;
	display: inline-block;
	width: 18px;
	height: 24px;
	cursor: pointer;
}
.wd-editImgUp:hover{
	background-image: url(images/arrowUp_hover.png);
}
.wd-eidtUnit{
	margin-left: 5px;
}
.wd-eidtWidth40{
	width: 40px;
}
.eidtImg-edit{
	color: #0066CC;
	text-decoration: underline;
	cursor: pointer;
}
.wd-editeditbox .wd-editp{
	margin: 0;
	padding: 0;
	word-break: break-all;
	cursor: text;
	position: relative;
}
.wd-editp .divbox{
	display: inline-block;
	float: none;
}
