		<script>
			$(function() {

				//复选框与单选框
				addWdCheckBox("#ckLayout");

				//下拉框插件
				function initSelectInput(key, obj) {
					if(key) {
						var keyArr = key.split('_');
						var keyArr_key = keyArr[1].toUpperCase();
					}
					var _mbList = [];
					if(typeof obj == "object") {
						for(var i = 0; i < obj.length; i++) {
							var temp = {
								"code": obj[i].code,
								"codedesc": obj[i].name
							};
							_mbList.push(temp);
						}
					}
					store.set(keyArr_key, JSON.stringify(_mbList));
					selecttextInfo();
				}
				initSelectInput.call(this, 'code_SFYX', [{
					'code': 1,
					'name': '全部'
				}, {
					'code': 2,
					'name': '待提交'
				}, {
					'code': 3,
					'name': '审核通过'
				}, {
					'code': 4,
					'name': '审核退回'
				}]);
				//筛选-点击搜索
				$(".hp-list-filter").click(function() {
						$(".hp-filter").slideDown("fast");
						var searchText = $("#wjfl").val() + "、" + getWDCodeDesc("code_SHZT");
						$(".hp-list-filter").html(searchText);
						$(".hp-list-filter").addClass("pink");
					})
					//筛选-点击搜索
				$("#tm-search").click(function() {
					$(".hp-filter").slideUp("fast");
					var searchText = $("#wjfl").val() + "、" + getWDCodeDesc("code_SHZT");
					$(".hp-list-filter").html(searchText);
					$(".hp-list-filter").addClass("pink");
				})
			})
		</script>
