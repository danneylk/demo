<script>
//angular路由
var app=angular.module('test', ["ngRoute", "userMod"]);
app.config(function ($routeProvider){
	$routeProvider
	.when( '/anzhuang', { templateUrl:'Basic/begin.html', controller:'anzhuang' } )
	.when( '/grid', { templateUrl: 'Basic/grid.html', controller:'biaoge'})
	.when( '/layout', { templateUrl: 'Basic/5layout.html', controller:'biaoge'})
	.when( '/paiban', { templateUrl: 'Basic/1paiban.html', controller:'paiban' } )
	.when( '/biaoge', { templateUrl: 'Basic/4table.html', controller:'biaoge' } )
	.when( '/color', { templateUrl: 'Basic/2color.html' } )
	.when( '/adminite', { templateUrl: 'Basic/6adminite.html', controller:'userController' } )
	.when( '/form', { templateUrl: 'form/form-xiala.html', controller:'form' } )
	.when( '/button', { templateUrl: 'Basic/3button.html', controller:'button' } )
	.when( '/Editor', { templateUrl: 'form/form-Editor.html', controller:'Editor' } )
	.when( '/tanchuang', { templateUrl: 'jsmodule/Alert.html', controller:'tanchaung' } )
	.when( '/Progress', { templateUrl: 'jsmodule/Progress.html', controller:'Progress' } )
	.when( '/mobile', { templateUrl: 'Basic/mobile.html',controller:'mobile'})
	.when( '/wuitab', { templateUrl: 'Basic/7tab.html',controller:'wuitab'})
	.when( '/footer', { template: '<footer class="footer mt-20"> <div class="container-fluid"> <nav> <a href="#" target="_blank">关于我们</a> <span class="pipe">|</span> <a href="#" target="_blank">联系我们</a> <span class="pipe">|</span> <a href="#" target="_blank">法律声明</a> </nav> <p>Copyright &copy;2016 wd.com All Rights Reserved. <br> <a href="http://www.miitbeian.gov.cn/" target="_blank" rel="nofollow">京ICP备1000000号</a><br> </p> </div> </footer>'})
	.otherwise({ redirectTo: '/anzhuang' });
});
</script>
<script>
//滚动条
$("ul").niceScroll({  
cursorcolor:"hsla(220,8%,56%,.3)",  
cursoropacitymax:1,  
touchbehavior:false,  
cursorwidth:"5px",  
cursorborder:"0",  
cursorborderradius:"5px"  
}); 
//左侧导航吸顶
$(document).scroll(function() {
	var scrollTop = window.pageYOffset || document.documentElement.scrollTop || document.body.scrollTop || 0;
	scrollTop > 50 ? $('.dofix').addClass('tabfix') : $('.dofix').removeClass('tabfix');
})
